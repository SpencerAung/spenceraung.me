{
  "name": "spenceraung.me",
  "description": "Spencer's Blog",
  "engines": {
    "node": ">=10.15.0"
  },
  "version": "1.0.0",
  "dependencies": {
    "babel-preset-gatsby": "^0.2.1",
    "gatsby": "^2.13.12",
    "gatsby-image": "^2.2.4",
    "gatsby-plugin-google-analytics": "^2.1.4",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-manifest": "^2.2.1",
    "gatsby-plugin-offline": "^2.2.1",
    "gatsby-plugin-react-helmet": "^3.1.0",
    "gatsby-plugin-sharp": "^2.2.3",
    "gatsby-plugin-styled-components": "^3.1.0",
    "gatsby-plugin-typography": "^2.3.0",
    "gatsby-remark-copy-linked-files": "^2.1.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.1.3",
    "gatsby-remark-prismjs": "^3.3.1",
    "gatsby-remark-static-images": "^1.1.0",
    "gatsby-source-filesystem": "^2.1.3",
    "gatsby-transformer-remark": "^2.6.1",
    "gatsby-transformer-sharp": "^2.2.1",
    "prismjs": "^1.15.0",
    "prop-types": "^15.6.2",
    "ramda": "^0.26.1",
    "ramda-extension": "^0.8.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-typography": "^0.16.19",
    "styled-components": "^4.3.2",
    "typography": "^0.16.19",
    "typography-theme-irving": "^0.16.19"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf node_modules",
    "reinstall": "npm run clean && npm install",
    "build": "gatsby build",
    "dev": "gatsby develop",
    "start": "npm run develop",
    "format": "standard --fix",
    "format:md": "standard --fix --plugin markdown",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.2",
    "eslint": "^6.1.0",
    "eslint-plugin-markdown": "^1.0.0",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.1",
    "standard": "^13.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SpencerAung/spenceraung.me.git"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "git add"
    ],
    "*.md": [
      "npm run format:md",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "build"
    ]
  }
}
